{
  "name": "CinePOS_FrontEnd",
  "version": "0.0.21",
  "scripts": {
    "ng": "ng",
    "start:front": "ng serve staff --port 4202 --host 0.0.0.0",
    "start:back": "ng serve manager --port 4203 --host 0.0.0.0",
    "move-404": "node postbuild.js",
    "patchVersion": "npm --no-git-tag-version version patch",
    "build:front:patch": "npm run patchVersion && ng build staff --base-href /CinePOS_FrontEnd/staff/ --configuration production --output-hashing=all && npm run move-404",
    "build:back:patch": "npm run patchVersion && ng build manager --base-href /CinePOS_FrontEnd/manager/ --configuration production --output-hashing=all && npm run move-404",
    "build:patch": "npm run patchVersion && ng build staff --base-href /CinePOS_FrontEnd/staff/ --configuration production && ng build manager --base-href /CinePOS_FrontEnd/manager/ --configuration production --output-hashing=all && npm run move-404",
    "deploy": "npx angular-cli-ghpages --dir=dist/CinePOS_FrontEnd",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "swagger:get-staff": "java -jar swagger-codegen-cli-3.0.31.jar generate -i https://api-t.cine-pos.com/v1/swagger.json -l typescript-angular -o ./projects/staff/src/app/api/cinePOS-api -D io.swagger.v3.parser.util.RemoteUrl.trustAll=true",
    "swagger:get-manager": "java -jar swagger-codegen-cli-3.0.31.jar generate -i https://api-t.cine-pos.com/v1/swagger.json -l typescript-angular -o ./projects/manager/src/app/api/cinePOS-api -D io.swagger.v3.parser.util.RemoteUrl.trustAll=true",
    "swagger:get-share-libs": "java -jar swagger-codegen-cli-3.0.31.jar generate -i https://api-t.cine-pos.com/v1/swagger.json -l typescript-angular -o ./projects/share-libs/src/lib/api/cinePOS-api -D io.swagger.v3.parser.util.RemoteUrl.trustAll=true"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "angular-cli-ghpages": "^1.0.6",
    "jwt-decode": "^3.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.11",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "^14.2.0",
    "@nguniversal/express-engine": "^14.2.3",
    "@types/jasmine": "~4.0.0",
    "autoprefixer": "^10.4.14",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "~4.7.2"
  }
}
